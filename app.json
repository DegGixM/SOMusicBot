{
  "name": "SO Music Bot",
  "description": "Telegram'ınızın Grup Sesli Sohbeti'nde musiqi oxumaq üçünn Açıq Kaynaklı bot. Powered by @MUCVE_M",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/offlineflood/SOMusicBot",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Piroqram sessiya sətri (Replit.com) ",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "@botfather ilə bot nişanı əldə edin",
      "required": true
    },
    "BOT_NAME": {
      "description": "Musiqi Pleyerinizin Bot Adı.",
      "required": false,
      "value": ""
    },
    "BOT_USARNAME": {
      "description": "Musiqi Pleyeri Bot İstifadəçi adını @ olmadan yazın.",
      "required": true,
      "value": ""
     
    },
    "API_ID": {
      "description": "my.telegram.org/apps saytından tətbiq identifikatoru",
      "required": true
    },
    "API_HASH": {
      "description": "my.telegram api-hash.org/apps saytından tətbiq",
      "required": true
    },
    "SUDO_USERS": {
      "description": "Hər yerdə admin sayılan istifadəçi identifikatorlarının siyahısı (Musiqi hesabı id).",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Yükləmələr üçün maksimum audio vaxt limiti (dəqiqə).",
      "required": true,
      "value": "90"
    }
  }
}
